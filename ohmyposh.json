{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "white",
          "style": "plain",
          "template": "@{{ .UserName }} ",
          "type": "session"
        },
        {
          "foreground": "white",
          "style": "plain",
          "template": "\u279c",
          "type": "text"
        },
        {
          "foreground": "green",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " {{ .Path }}",
          "type": "path"
        },
        {
          "foreground": "green",
          "properties": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": " <white>git(</>{{ .HEAD }}<white>)</>",
          "type": "git"
        },
        {
          "foreground": "cyan",
          "style": "plain",
          "template": " \uf119",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
